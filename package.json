{
    "name": "redux-store-generator",
    "version": "0.9.62",
    "description": "auto-generates a redux store",
    "license": "MIT",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "start": "npm link",
        "test": "jest",
        "clean": "rm -rf lib/*",
        "build": "npm run clean && tsc",
        "watch": "nodemon -x \"npm run build\"",
        "analyze": "source-map-explorer 'lib/*.js'  --no-border-checks"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "files": [
        "lib/**/*"
    ],
    "author": "",
    "dependencies": {
        "@types/chance": "^1.1.1"
    },
    "devDependencies": {
        "@types/jest": "^26.0.21",
        "chance": "^1.1.7",
        "jest": "^26.6.3",
        "source-map-explorer": "^2.5.2",
        "ts-jest": "^26.5.0",
        "typescript": "^4.1.3"
    }
}
